<?xml version="1.0"?>
<device>
  <developer>panStamp</developer>
  <product>Dual Temperature/Humidity sensor with 8 binary switches</product>
  <pwrdownmode>true</pwrdownmode>
  <regular>
    <reg name="Voltage, Humidity, Temperature and binary information" id="11">
      <endpoint name="Voltage" type="num" dir="inp">
        <position>0</position>
        <size>2</size>
        <units>
          <unit name="V" factor="0.001" offset="0"/>
        </units>
      </endpoint>
      <endpoint name="Temperature" type="num" dir="inp">
        <position>2</position>
        <size>2</size>
        <units>
          <unit name="C" factor="0.1" offset="-50"/>
          <unit name="F" factor="0.18" offset="-58"/>
          <unit name="K" factor="0.1" offset="223.15"/>
        </units>
      </endpoint>
      <endpoint name="Humidity" type="num" dir="inp">
        <position>4</position>
        <size>2</size>
        <units>
          <unit name="%" factor="0.1" offset="0"/>
        </units>
      </endpoint>
      <endpoint name="Binary 0" type="bin" dir="inp">
        <position>6.0</position>
        <size>0.1</size>
      </endpoint>
      <endpoint name="Binary 1" type="bin" dir="inp">
        <position>6.1</position>
        <size>0.1</size>
      </endpoint>
      <endpoint name="Binary 2" type="bin" dir="inp">
        <position>6.2</position>
        <size>0.1</size>
      </endpoint>
      <endpoint name="Binary 3" type="bin" dir="inp">
        <position>6.3</position>
        <size>0.1</size>
      </endpoint>
      <endpoint name="Binary 4" type="bin" dir="inp">
        <position>6.4</position>
        <size>0.1</size>
      </endpoint>
      <endpoint name="Binary 5" type="bin" dir="inp">
        <position>6.5</position>
        <size>0.1</size>
      </endpoint>
      <endpoint name="Binary 6" type="bin" dir="inp">
        <position>6.6</position>
        <size>0.1</size>
      </endpoint>
      <endpoint name="Binary 7" type="bin" dir="inp">
        <position>6.7</position>
        <size>0.1</size>
      </endpoint>
    </reg>
  </regular>
</device>

